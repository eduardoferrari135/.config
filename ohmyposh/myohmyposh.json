{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "bg_dim": "#141617",
    "bg0": "#1D2021",
    "bg1": "#282828",
    "bg2": "#282828",
    "bg3": "#3C3836",
    "bg4": "#3C3836",
    "bg5": "#504945",
    "bg_diff_red": "#3C1F1E",
    "bg_diff_green": "#32361A",
    "bg_diff_blue": "#0D3138",
    "bg_statusline1": "#282828",
    "bg_statusline2": "#32302F",
    "bg_statusline3": "#504945",
    "bg_current_word": "#32302F",
    "bg_visual_red": "#442E2D",
    "bg_visual_green": "#333E34",
    "bg_visual_blue": "#2E3B3B",
    "bg_visual_yellow": "#473C29",
    "bg_visual_purple": "#3C333B",
    "fg0": "#D4BE98",
    "fg1": "#DDC7A1",
    "red": "#EA6962",
    "green": "#A9B665",
    "blue": "#7DAEA3",
    "yellow": "#D8A657",
    "purple": "#D3869B",
    "orange": "#E78A4E",
    "aqua": "#89B482",
    "bg_red": "#EA6962",
    "bg_green": "#A9B665",
    "bg_yellow": "#D8A657",
    "grey0": "#7C6F64",
    "grey1": "#928374",
    "grey2": "#A89984"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{.Icon}} ",
          "foreground": "p:bg0",
          "background": "p:aqua",
          "type": "os"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ .UserName }}@{{ .HostName }} ",
          "foreground": "p:bg0",
          "powerline_symbol": "\ue0b0",
          "background": "p:aqua",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "..",
            "home_icon": "~",
            "style": "agnoster"
          },
          "template": " {{ .Path }} ",
          "foreground": "p:bg0",
          "powerline_symbol": "\ue0b0",
          "background": "p:red",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "cache_duration": "none",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": " {{ .HEAD }} {{if .BranchStatus }} {{ .BranchStatus }} {{ end }}{{ if .Working.Changed }} {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} {{ .StashCount }}{{ end }}",
          "foreground": "p:bg0",
          "powerline_symbol": "\ue0b0",
          "background": "p:green",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:yellow{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:yellow{{ end }}",
            "{{ if gt .Ahead 0 }}p:purple{{ end }}",
            "{{ if gt .Behind 0 }}p:purple{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "files",
            "fetch_virtual_env": true
          },
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}{{ if .Venv }} ({{ .Venv }}){{ end }} ",
          "foreground": "p:bg0",
          "powerline_symbol": "\ue0b0",
          "background": "p:yellow",
          "type": "python",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue77f {{ .Full }} ",
          "foreground": "p:bg0",
          "powerline_symbol": "\ue0b0",
          "background": "p:purple",
          "type": "dotnet",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \udb81\udfd3Â  {{ .Full }} ",
          "foreground": "p:bg0",
          "powerline_symbol": "\ue0b0",
          "background": "p:blue",
          "type": "go",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue755 {{ .Full }} ",
          "foreground": "p:bg0",
          "powerline_symbol": "\ue0b0",
          "background": "p:orange",
          "type": "swift",
          "style": "powerline"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
